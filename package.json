    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.3",
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "@mdx-js/loader": "^0.17.5",
    "@mdx-js/mdx": "^0.15.0",
    "@mdx-js/tag": "^0.17.5",
    "@types/lodash": "^4.14.123",
